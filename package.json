{
  "name": "rbc-app",
  "version": "0.0.1",
  "description": "The public facing RbC app",
  "productName": "Regenerative by Community app",
  "author": "Michael Kelly <mike@curioushuman.com.au>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "build": "quasar build",
    "lint": "eslint --ext .js,.ts,.vue --fix ./ && echo 'Lint complete.'",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:e2e": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress open\"",
    "test:e2e:ci": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress run\"",
    "test:unit": "cypress open-ct",
    "test:unit:ci": "cypress run-ct",
    "test:coverage:text": "nyc --reporter=text npm run test:unit:ci",
    "test:coverage:html": "nyc --reporter=html --report-dir='./coverage' npm run test:unit:ci"
  },
  "dependencies": {
